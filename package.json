{
	"name": "tanstack-ripple-query",
	"version": "0.0.1",
	"description": "Helpers for fetching, caching and updating asynchronous data in Ripple.",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wobsoriano/tanstack-ripple-query.git"
	},
